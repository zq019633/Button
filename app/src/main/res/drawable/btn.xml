<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">         /
    <item android:state_pressed="true">//定义当button 处于pressed 状态时的形态。  点击后
        <shape>
            <corners android:topLeftRadius="20dp"
                android:bottomLeftRadius="20dp"        />
            <stroke   android:width="0.1dp" android:color="#00ffff" />

        </shape>
    </item>



    <item android:state_focused="false">//定义当button获得 focus时的形态   点击前
        <shape>
            <!--填充-->
            <solid android:color="#00ffff"></solid>
            <corners android:topLeftRadius="20dp"
                android:bottomLeftRadius="20dp"       />
        </shape>
    </item>
</selector>